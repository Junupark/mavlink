      <entry value = "0" name="ASCL_COMMAND_NAV_RESET_COVARIANCE_FROM_TO">
        <description></description>
      </entry>
      <entry value = "0" name="ASCL_COMMAND_NAV_RESET_COVARIANCE_FROM_TO">
        <description></description>
